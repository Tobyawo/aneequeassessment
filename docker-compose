version: '3.3'
services:
  aneeque_api:
    container_name: aneeque_fashion
    image: aneeque_fashion
    build: ./
    ports:
      - "8045:8080"
    depends_on:
      - postgresqldb
  postgresqldb:
    image: postgres
    ports:
      - "5440:5432"
    environment:
      - POSTGRES_PASSWORD=aneeque
      - POSTGRES_USER=postgres
      - POSTGRES_DB=aneeque_DB